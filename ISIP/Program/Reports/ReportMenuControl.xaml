<UserControl x:Class="ISIP.Program.Reports.ReportMenuControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ISIP.Program.Reports"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ListBox Name="ReportList" ItemContainerStyle="{StaticResource MainMenutListBoxItem}" Background="#FF641A68" BorderThickness="0" SelectionChanged="ReportList_SelectionChanged"/>
    </Grid>
</UserControl>
