<UserControl x:Class="ISIP.Program.Collections.CollectionMenuControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ISIP.Program.Collections"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ListBox Name="CollectionList" ItemContainerStyle="{StaticResource MainMenutListBoxItem}" Background="#FF641A68" BorderThickness="0" SelectionChanged="CollectionList_SelectionChanged" />
    </Grid>
</UserControl>
